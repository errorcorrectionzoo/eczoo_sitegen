{
  "name": "@errorcorrectionzoo/site",
  "version": "0.0.1",
  "packageManager": "yarn@3.3.0",
  "config": {
    "siteLandingPaths": [
      "index.html",
      "**/*.html",
      "schema/*.json",
      "dat/*.json"
    ]
  },
  "private": true,
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@errorcorrectionzoo/eczoodb": ">=0.0.1",
    "@errorcorrectionzoo/jscomponents": ">=0.0.1",
    "@kitschpatrol/eleventy-plugin-parcel": "https://github.com/phfaist/eleventy-plugin-parcel.git",
    "@phfaist/zoodb": "https://github.com/phfaist/zoodb.git#main",
    "debug": "^4.3.4",
    "eleventy-favicon": "^1.1.3",
    "parcel": "^2.8.3",
    "parcel-optimizer-nohtmlsuffix": "https://github.com/phfaist/parcel-optimizer-nohtmlsuffix",
    "sirv-cli": "^2.0.2"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.8.3",
    "@parcel/transformer-sass": "2.8.3"
  },
  "scripts": {
    "build": "rm -rf _site && DEBUG=\"*Error*,*zoo*,Eleventy:Logger,Eleventy:Benchmark\" eleventy",
    "build-simple": "rm -rf _site && ECZOO_DEVELOPMENT_MODE=1 DEBUG=\"*Error*,*zoo*,Eleventy:Logger,Eleventy:Benchmark\" eleventy",
    "build-test": "rm -rf _site && ECZOO_USE_TEST_DATA=1 DEBUG=\"*Error*,*zoo*,Eleventy*\" eleventy",
    "dev": "rm -rf _site && ECZOO_DEVELOPMENT_MODE=1 DEBUG=\"*Error*,*zoo*,Eleventy:Logger,Eleventy:Benchmark\" eleventy --serve",
    "dev-test": "rm -rf _site && ECZOO_USE_TEST_DATA=1 ECZOO_DEVELOPMENT_MODE=1 DEBUG=\"*Error*,*zoo*,Eleventy*\" eleventy --serve",
    "serve": "sirv _site/"
  },
  "@mischnic/parcel-resolver-root": {
    "/": "./_site",
    "~": ".."
  }
}
