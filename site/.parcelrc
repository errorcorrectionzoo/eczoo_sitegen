{
    "extends": "@parcel/config-default",

    "resolvers": [
        "@mischnic/parcel-resolver-root",
        "...",
    ],

    "transformers": {
        "*.{pdf,jpg,jpeg,png}": ["@parcel/transformer-raw"],
        "*.{json,ico}": ["@parcel/transformer-raw"],
        "*.{xml,txt}": ["@parcel/transformer-raw"],
    },

    "optimizers": {
        "*.html": ["parcel-optimizer-nohtmlsuffix", "..."]
    },

}
